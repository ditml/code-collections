<!DOCTYPE html>
<html>
  <style>
    body {
      background-color: skyblue;
      font-family: verdana;
      color: white;
      padding: 30px;
      text-align: center;
    }
    h1 {
      font-size: 48px;
      text-transform: uppercase;
      letter-spacing: 2px;
    }
    p {
      font-size: 16px;
    }
  </style>

  <head></head>
  <body>
    <%- include ('partials/nav.ejs') %>
    <h1>Welcome to the profile of <%= person %></h1>
    <p><strong>Age: </strong><%= data.age %></p>
    <p><strong>Place: </strong><%= data.place %></p>
    <h2>Hobbies</h2>
    <ul>
      <% data.hobbies.forEach(function(item) { %>
        <li><%= item %></li>
      <% }); %>
    </ul>
  </body>
</html>